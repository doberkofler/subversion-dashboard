{
  "name": "subversion-dashboard",
  "version": "0.0.1",
  "description": "Express based dashboard to monitor Subversion repositories.",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.0",
    "@mui/x-data-grid": "5.17.14",
    "compression": "1.7.4",
    "date-fns": "2.29.3",
    "debug": "4.3.4",
    "express": "4.18.2",
    "install": "^0.13.0",
    "node-schedule": "2.1.0",
    "npm": "^9.2.0",
    "ping": "0.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.5",
    "threads": "1.7.0",
    "tslib": "2.4.1",
    "xml2js": "0.4.23",
    "yargs": "17.6.2",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@tsconfig/node18-strictest": "1.0.0",
    "@types/compression": "1.7.2",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.15",
    "@types/fs-extra": "9.0.13",
    "@types/glob": "8.0.0",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.15",
    "@types/node-schedule": "2.1.0",
    "@types/ping": "0.4.1",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/supertest": "2.0.12",
    "@types/xml2js": "0.4.11",
    "@types/yargs": "17.0.17",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "esbuild": "0.16.7",
    "eslint": "8.29.0",
    "eslint-plugin-jest": "27.1.6",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "fs-extra": "^11.1.0",
    "jest": "29.3.1",
    "nodemon": "2.0.20",
    "rimraf": "3.0.2",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "tsutils": "3.21.0",
    "typescript": "4.9.4"
  },
  "scripts": {
    "start": "nodemon --watch \"out/**\" --watch \"config/**\" --ext \"*\" out/server/index.js --verbose --config=./config/config.json",
    "clean": "rimraf coverage build tmp out && jest --clearCache",
    "build": "npm run clean && ts-node scripts/build.ts",
    "build-watch": "npm run clean && nodemon --watch \"src/**\" --ext \"*\" --exec \"ts-node scripts/build.ts\"",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives && tsc --noEmit && tsc --noEmit --project ./scripts/tsconfig.json",
    "test": "jest --coverage=false",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --coverage=false",
    "createTestRepo": "node out/createTestRepo.js"
  },
  "author": "Dieter Oberkofler <doberkofler@users.noreply.github.com",
  "license": "Apache-2.0"
}
